class unggas {
    private String nama;
    private String warna;
    private int umur;

    public unggas() {
    }

    public unggas(String nama, String warna, int umur) {
        this.nama = nama;
        this.warna = warna;
        this.umur = umur;
    }

    public String getNama() {
        return nama;
    }

    public void setNama(String nama) {
        this.nama = nama;
    }

    public String getWarna() {
        return warna;
    }

    public void setWarna(String warna) {
        this.warna = warna;
    }

    public int getUmur() {
        return umur;
    }

    public void setUmur(int umur) {
        this.umur = umur;
    }

    public void suara() {
        System.out.println("Unggas bersuara.");
    }

    @Override
    public String toString() {
        return "Nama: " + nama + ", Warna: " + warna + ", Umur: " + umur;
    }
}
